<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- PAGES -->
    <sys:String x:Key="Languages">Languages</sys:String>
    <sys:String x:Key="SettingsTitle">Settings</sys:String>
    <sys:String x:Key="FAQS">FAQS</sys:String>

    <!-- GENERAL -->
    <sys:String x:Key="ConfirmExit">Do you want to exit?</sys:String>
    <sys:String x:Key="LogOut">Log Out</sys:String>
    <sys:String x:Key="Yes">Yes</sys:String>
    
    <sys:String x:Key="Administrators">Administrators</sys:String>
    <sys:String x:Key="Artists">Artists</sys:String>
    <sys:String x:Key="Spaces">Spaces</sys:String>

    <sys:String x:Key="Username">Username</sys:String>
    <sys:String x:Key="Password">Password</sys:String>
    <sys:String x:Key="LogIn">Log In</sys:String>

    <sys:String x:Key="Hour">Hour</sys:String>
    <sys:String x:Key="Event">Event</sys:String>

    <sys:String x:Key="Dashboard">Dashboard</sys:String>
    <sys:String x:Key="EventsToday">Events Today</sys:String>
    <sys:String x:Key="TotalSuperAdmins">SuperAdmins</sys:String>
    <sys:String x:Key="TotalAdmins">Admins</sys:String>
    <sys:String x:Key="TotalMaintenanceUsers">Maintenance Users</sys:String>
    <sys:String x:Key="TotalAppUsers">App Users</sys:String>
    <sys:String x:Key="TotalArtists">Artists</sys:String>
    <sys:String x:Key="TotalSpaces">Spaces</sys:String>
    <sys:String x:Key="TotalMatches">Matches</sys:String>
    <sys:String x:Key="TotalEventsDone">Events Done</sys:String>

    <!-- FAQ TRANSLATIONS -->
    <sys:String x:Key="Faq1Question">How can I track my shipment?</sys:String>
    <sys:String x:Key="Faq1Answer">Use your tracking ID in the shipment portal.</sys:String>

    <sys:String x:Key="Faq2Question">What should I do if my package is delayed?</sys:String>
    <sys:String x:Key="Faq2Answer">Check the tracking details or contact support.</sys:String>

    <sys:String x:Key="Faq3Question">Can I change my delivery address?</sys:String>
    <sys:String x:Key="Faq3Answer">Yes, within 24 hours of placing the order.</sys:String>

</ResourceDictionary>