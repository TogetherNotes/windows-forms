<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- PAGES -->
    <sys:String x:Key="Languages">Idiomas</sys:String>
    <sys:String x:Key="SettingsTitle">Configuración</sys:String>
    <sys:String x:Key="FAQS">Preguntas Frequentes</sys:String>

    <!-- GENERAL -->
    <sys:String x:Key="ConfirmExit">¿Quieres salir?</sys:String>
    <sys:String x:Key="LogOut">Salir</sys:String>
    <sys:String x:Key="Yes">Sí</sys:String>
    
    <sys:String x:Key="Administrators">Administradores</sys:String>
    <sys:String x:Key="Artists">Artistas</sys:String>
    <sys:String x:Key="Spaces">Espacios</sys:String>
    
    <sys:String x:Key="Username">Usuario</sys:String>
    <sys:String x:Key="Password">Contraseña</sys:String>
    <sys:String x:Key="LogIn">Iniciar sesión</sys:String>

    <sys:String x:Key="Hour">Hora</sys:String>
    <sys:String x:Key="Event">Evento</sys:String>

    <sys:String x:Key="Search">Buscar usuario</sys:String>

    <!-- FAQ TRANSLATIONS -->
    <sys:String x:Key="Faq1Question">¿Cómo puedo rastrear mi envío?</sys:String>
    <sys:String x:Key="Faq1Answer">Usa tu ID de rastreo en el portal de envíos.</sys:String>

    <sys:String x:Key="Faq2Question">¿Qué hago si mi paquete se retrasa?</sys:String>
    <sys:String x:Key="Faq2Answer">Revisa los detalles de seguimiento o contacta con soporte.</sys:String>

    <sys:String x:Key="Faq3Question">¿Puedo cambiar mi dirección de entrega?</sys:String>
    <sys:String x:Key="Faq3Answer">Sí, dentro de las primeras 24 horas tras realizar el pedido.</sys:String>

</ResourceDictionary>