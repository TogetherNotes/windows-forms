<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:TogetherNotes.ViewModel"
                    xmlns:view="clr-namespace:TogetherNotes.Forms">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CalendarVM}">
        <view:Calendar/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MapVM}">
        <view:Map/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <view:Settings/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FaqsVM}">
        <view:Faqs/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LoginVM}">
        <view:Login/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ManageAdminVM}">
        <view:ManageAdmin/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ManageAppVM}">
        <view:ManageApp/>
    </DataTemplate>

</ResourceDictionary>