<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:TogetherNotes.ViewModel"
                    xmlns:view="clr-namespace:TogetherNotes.Forms"
                    >

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CustomerVM}">
        <view:Customers/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductVM}">
        <view:Products/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:OrderVM}">
        <view:Orders/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TransactionVM}">
        <view:Transactions/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ShipmentVM}">
        <view:Shipment/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <view:Settings/>
    </DataTemplate>

</ResourceDictionary>
    